<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
   <servlet>
        <servlet-name>content</servlet-name>
        <servlet-class>EclipseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>content</servlet-name>
        <url-pattern>ie/content/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>content</servlet-name>
        <url-pattern>ns4/content/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>content</servlet-name>
        <url-pattern>ns6/content/*</url-pattern>
    </servlet-mapping>
</web-app>
